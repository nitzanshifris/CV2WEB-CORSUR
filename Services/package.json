{
  "name": "cv2web-services",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "dev": "nodemon",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' nodemon",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "validate": "npm-run-all --parallel type-check lint test"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "prisma": "^5.0.0",
    "rate-limiter-flexible": "^3.0.0",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/morgan": "^1.9.0",
    "@types/swagger-ui-express": "^4.1.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "nodemon": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0"
  }
}
